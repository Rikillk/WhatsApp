\cf2 \cb3 \strokec2 # 'Main' server configuration\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # The directives in this section set up the values used by the 'main'\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # server, which responds to any requests that aren't handled by a\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # <VirtualHost> definition.  These values also provide defaults for\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # any <VirtualHost> containers you may define later in the file.\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # All of these directives may appear inside <VirtualHost> containers,\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # in which case these default settings will be overridden for the\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # virtual host being defined.\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # ServerAdmin: Your address, where problems with the server should be\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # e-mailed.  This address appears on some server-generated pages, such\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # as error documents.  e.g. admin@your-domain.com\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\pard\pardeftab720\partightenfactor0
\cf4 \cb3 ServerAdmin you@example.com\cb1 \
\
\pard\pardeftab720\partightenfactor0
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # ServerName gives the name and port that the server uses to identify itself.\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # This can often be determined automatically, but we recommend you specify\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # it explicitly to prevent problems during startup.\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # If your host doesn't have a registered DNS name, enter its IP address here.\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #ServerName www.example.com:8080\cf4 \cb1 \strokec4 \
\
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # Deny access to the entirety of your server's filesystem. You must\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # explicitly permit access to web content directories in other \cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # <Directory> blocks below.\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\pard\pardeftab720\partightenfactor0
\cf4 \cb3 <Directory />\cb1 \
\cb3     AllowOverride All\cb1 \
\cb3     Require all granted\cb1 \
\cb3 </Directory>\cb1 \
\
\pard\pardeftab720\partightenfactor0
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # Note that from this point forward you must specifically allow\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # particular features to be enabled - so if something's not working as\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # you might expect, make sure that you have specifically enabled it\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # below.\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # DocumentRoot: The directory out of which you will serve your\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # documents. By default, all requests are taken from this directory, but\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # symbolic links and aliases may be used to point to other locations.\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\pard\pardeftab720\partightenfactor0
\cf4 \cb3 DocumentRoot \cf5 \strokec5 "/usr/local/var/www"\cf4 \cb1 \strokec4 \
\cb3 <Directory \cf5 \strokec5 "/usr/local/var/www"\cf4 \strokec4 >\cb1 \
\cb3     \cf2 \strokec2 #\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # Possible values for the Options directive are "None", "All",\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # or any combination of:\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 #   Indexes Includes FollowSymLinks SymLinksifOwnerMatch ExecCGI MultiViews\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 #\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # Note that "MultiViews" must be named *explicitly* --- "Options All"\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # doesn't give it to you.\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 #\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # The Options directive is both complicated and important.  Please see\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # http://httpd.apache.org/docs/2.4/mod/core.html#options\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # for more information.\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 #\cf4 \cb1 \strokec4 \
\cb3     Options Indexes FollowSymLinks\cb1 \
\
\cb3     \cf2 \strokec2 #\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # AllowOverride controls what directives may be placed in .htaccess files.\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # It can be "All", "None", or any combination of the keywords:\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 #   AllowOverride FileInfo AuthConfig Limit\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 #\cf4 \cb1 \strokec4 \
\cb3     AllowOverride None\cb1 \
\
\cb3     \cf2 \strokec2 #\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # Controls who can get stuff from this server.\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 #\cf4 \cb1 \strokec4 \
\cb3     Require all granted\cb1 \
\cb3 </Directory>\cb1 \
\
\pard\pardeftab720\partightenfactor0
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # DirectoryIndex: sets the file that Apache will serve if a directory\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # is requested.\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\pard\pardeftab720\partightenfactor0
\cf4 \cb3 <IfModule dir_module>\cb1 \
\cb3    DirectoryIndex index.html index.php\cb1 \
\cb3 </IfModule>\cb1 \
\
\cb3 <FilesMatch \\.php$>\cb1 \
\cb3     SetHandler application/x-httpd-php\cb1 \
\cb3 </FilesMatch>\cb1 \
\
\pard\pardeftab720\partightenfactor0
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # The following lines prevent .htaccess and .htpasswd files from being \cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # viewed by Web clients. \cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\pard\pardeftab720\partightenfactor0
\cf4 \cb3 <Files \cf5 \strokec5 ".ht*"\cf4 \strokec4 >\cb1 \
\cb3     Require all granted\cb1 \
\cb3 </Files>\cb1 \
\
\pard\pardeftab720\partightenfactor0
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # ErrorLog: The location of the error log file.\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # If you do not specify an ErrorLog directive within a <VirtualHost>\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # container, error messages relating to that virtual host will be\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # logged here.  If you *do* define an error logfile for a <VirtualHost>\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # container, that host's errors will be logged there and not here.\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\pard\pardeftab720\partightenfactor0
\cf4 \cb3 ErrorLog \cf5 \strokec5 "/usr/local/var/log/httpd/error_log"\cf4 \cb1 \strokec4 \
\
\pard\pardeftab720\partightenfactor0
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # LogLevel: Control the number of messages logged to the error_log.\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # Possible values include: debug, info, notice, warn, error, crit,\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # alert, emerg.\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\pard\pardeftab720\partightenfactor0
\cf4 \cb3 LogLevel warn\cb1 \
\
\cb3 <IfModule log_config_module>\cb1 \
\cb3     \cf2 \strokec2 #\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # The following directives define some format nicknames for use with\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # a CustomLog directive (see below).\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 #\cf4 \cb1 \strokec4 \
\cb3     LogFormat \cf5 \strokec5 "%h %l %u %t \\"\cf4 \strokec4 %r\\\cf5 \strokec5 " %>s %b \\"\cf4 \strokec4 %\{Referer\}i\\\cf5 \strokec5 " \\"\cf4 \strokec4 %\{User-Agent\}i\\\cf5 \strokec5 ""\cf4 \strokec4  combined\cb1 \
\cb3     LogFormat \cf5 \strokec5 "%h %l %u %t \\"\cf4 \strokec4 %r\\\cf5 \strokec5 " %>s %b"\cf4 \strokec4  common\cb1 \
\
\cb3     <IfModule logio_module>\cb1 \
\cb3       \cf2 \strokec2 # You need to enable mod_logio.c to use %I and %O\cf4 \cb1 \strokec4 \
\cb3       LogFormat \cf5 \strokec5 "%h %l %u %t \\"\cf4 \strokec4 %r\\\cf5 \strokec5 " %>s %b \\"\cf4 \strokec4 %\{Referer\}i\\\cf5 \strokec5 " \\"\cf4 \strokec4 %\{User-Agent\}i\\\cf5 \strokec5 " %I %O"\cf4 \strokec4  combinedio\cb1 \
\cb3     </IfModule>\cb1 \
\
\cb3     \cf2 \strokec2 #\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # The location and format of the access logfile (Common Logfile Format).\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # If you do not define any access logfiles within a <VirtualHost>\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # container, they will be logged here.  Contrariwise, if you *do*\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # define per-<VirtualHost> access logfiles, transactions will be\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # logged therein and *not* in this file.\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 #\cf4 \cb1 \strokec4 \
\cb3     CustomLog \cf5 \strokec5 "/usr/local/var/log/httpd/access_log"\cf4 \strokec4  common\cb1 \
\
\cb3     \cf2 \strokec2 #\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # If you prefer a logfile with access, agent, and referer information\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # (Combined Logfile Format) you can use the following directive.\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 #\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 #CustomLog "/usr/local/var/log/httpd/access_log" combined\cf4 \cb1 \strokec4 \
\cb3 </IfModule>\cb1 \
\
\cb3 <IfModule alias_module>\cb1 \
\cb3     \cf2 \strokec2 #\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # Redirect: Allows you to tell clients about documents that used to \cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # exist in your server's namespace, but do not anymore. The client \cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # will make a new request for the document at its new location.\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # Example:\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # Redirect permanent /foo http://www.example.com/bar\cf4 \cb1 \strokec4 \
\
\cb3     \cf2 \strokec2 #\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # Alias: Maps web paths into filesystem paths and is used to\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # access content that does not live under the DocumentRoot.\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # Example:\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # Alias /webpath /full/filesystem/path\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 #\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # If you include a trailing / on /webpath then the server will\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # require it to be present in the URL.  You will also likely\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # need to provide a <Directory> section to allow access to\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # the filesystem path.\cf4 \cb1 \strokec4 \
\
\cb3     \cf2 \strokec2 #\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # ScriptAlias: This controls which directories contain server scripts. \cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # ScriptAliases are essentially the same as Aliases, except that\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # documents in the target directory are treated as applications and\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # run by the server when requested rather than as documents sent to the\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # client.  The same rules about trailing "/" apply to ScriptAlias\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # directives as to Alias.\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 #\cf4 \cb1 \strokec4 \
\cb3     ScriptAlias /cgi-bin/ \cf5 \strokec5 "/usr/local/var/www/cgi-bin/"\cf4 \cb1 \strokec4 \
\
\cb3 </IfModule>\cb1 \
\
\cb3 <IfModule cgid_module>\cb1 \
\cb3     \cf2 \strokec2 #\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # ScriptSock: On threaded servers, designate the path to the UNIX\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # socket used to communicate with the CGI daemon of mod_cgid.\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 #\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 #Scriptsock cgisock\cf4 \cb1 \strokec4 \
\cb3 </IfModule>\cb1 \
\
\pard\pardeftab720\partightenfactor0
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # "/usr/local/var/www/cgi-bin" should be changed to whatever your ScriptAliased\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # CGI directory exists, if you have that configured.\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\pard\pardeftab720\partightenfactor0
\cf4 \cb3 <Directory \cf5 \strokec5 "/usr/local/var/www/cgi-bin"\cf4 \strokec4 >\cb1 \
\cb3     AllowOverride None\cb1 \
\cb3     Options None\cb1 \
\cb3     Require all granted\cb1 \
\cb3 </Directory>\cb1 \
\
\cb3 <IfModule headers_module>\cb1 \
\cb3     \cf2 \strokec2 #\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # Avoid passing HTTP_PROXY environment to CGI's on this or any proxied\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # backend servers which have lingering "httpoxy" defects.\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # 'Proxy' request header is undefined by the IETF, not listed by IANA\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 #\cf4 \cb1 \strokec4 \
\cb3     RequestHeader unset Proxy early\cb1 \
\cb3 </IfModule>\cb1 \
\
\cb3 <IfModule mime_module>\cb1 \
\cb3     \cf2 \strokec2 #\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # TypesConfig points to the file containing the list of mappings from\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # filename extension to MIME-type.\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 #\cf4 \cb1 \strokec4 \
\cb3     TypesConfig /usr/local/etc/httpd/mime.types\cb1 \
\
\cb3     \cf2 \strokec2 #\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # AddType allows you to add to or override the MIME configuration\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # file specified in TypesConfig for specific file types.\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 #\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 #AddType application/x-gzip .tgz\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 #\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # AddEncoding allows you to have certain browsers uncompress\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # information on the fly. Note: Not all browsers support this.\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 #\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 #AddEncoding x-compress .Z\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 #AddEncoding x-gzip .gz .tgz\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 #\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # If the AddEncoding directives above are commented-out, then you\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # probably should define those extensions to indicate media types:\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 #\cf4 \cb1 \strokec4 \
\cb3     AddType application/x-compress .Z\cb1 \
\cb3     AddType application/x-gzip .gz .tgz\cb1 \
\
\cb3     \cf2 \strokec2 #\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # AddHandler allows you to map certain file extensions to "handlers":\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # actions unrelated to filetype. These can be either built into the server\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # or added with the Action directive (see below)\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 #\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # To use CGI scripts outside of ScriptAliased directories:\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # (You will also need to add "ExecCGI" to the "Options" directive.)\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 #\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 #AddHandler cgi-script .cgi\cf4 \cb1 \strokec4 \
\
\cb3     \cf2 \strokec2 # For type maps (negotiated resources):\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 #AddHandler type-map var\cf4 \cb1 \strokec4 \
\
\cb3     \cf2 \strokec2 #\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # Filters allow you to process content before it is sent to the client.\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 #\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # To parse .shtml files for server-side includes (SSI):\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 # (You will also need to add "Includes" to the "Options" directive.)\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 #\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 #AddType text/html .shtml\cf4 \cb1 \strokec4 \
\cb3     \cf2 \strokec2 #AddOutputFilter INCLUDES .shtml\cf4 \cb1 \strokec4 \
\cb3 </IfModule>\cb1 \
\
\pard\pardeftab720\partightenfactor0
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # The mod_mime_magic module allows the server to use various hints from the\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # contents of the file itself to determine its type.  The MIMEMagicFile\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # directive tells the module where the hint definitions are located.\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #MIMEMagicFile /usr/local/etc/httpd/magic\cf4 \cb1 \strokec4 \
\
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # Customizable error responses come in three flavors:\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # 1) plain text 2) local redirects 3) external redirects\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # Some examples:\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #ErrorDocument 500 "The server made a boo boo."\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #ErrorDocument 404 /missing.html\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #ErrorDocument 404 "/cgi-bin/missing_handler.pl"\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #ErrorDocument 402 http://www.example.com/subscription_info.html\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # MaxRanges: Maximum number of Ranges in a request before\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # returning the entire resource, or one of the special\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # values 'default', 'none' or 'unlimited'.\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # Default setting is to accept 200 Ranges.\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #MaxRanges unlimited\cf4 \cb1 \strokec4 \
\
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # EnableMMAP and EnableSendfile: On systems that support it, \cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # memory-mapping or the sendfile syscall may be used to deliver\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # files.  This usually improves server performance, but must\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # be turned off when serving from networked-mounted \cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # filesystems or if support for these functions is otherwise\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # broken on your system.\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # Defaults: EnableMMAP On, EnableSendfile Off\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #EnableMMAP off\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #EnableSendfile on\cf4 \cb1 \strokec4 \
\
\cf2 \cb3 \strokec2 # Supplemental configuration\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # The configuration files in the /usr/local/etc/httpd/extra/ directory can be \cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # included to add extra features or to modify the default configuration of \cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # the server, or you may simply copy their contents here and change as \cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # necessary.\cf4 \cb1 \strokec4 \
\
\cf2 \cb3 \strokec2 # Server-pool management (MPM specific)\cf4 \cb1 \strokec4 \
\pard\pardeftab720\partightenfactor0
\cf4 \cb3 Include /usr/local/etc/httpd/extra/httpd-mpm.conf\cb1 \
\
\pard\pardeftab720\partightenfactor0
\cf2 \cb3 \strokec2 # Multi-language error messages\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #Include /usr/local/etc/httpd/extra/httpd-multilang-errordoc.conf\cf4 \cb1 \strokec4 \
\
\cf2 \cb3 \strokec2 # Fancy directory listings\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #Include /usr/local/etc/httpd/extra/httpd-autoindex.conf\cf4 \cb1 \strokec4 \
\
\cf2 \cb3 \strokec2 # Language settings\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #Include /usr/local/etc/httpd/extra/httpd-languages.conf\cf4 \cb1 \strokec4 \
\
\cf2 \cb3 \strokec2 # User home directories\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #Include /usr/local/etc/httpd/extra/httpd-userdir.conf\cf4 \cb1 \strokec4 \
\
\cf2 \cb3 \strokec2 # Real-time info on requests and configuration\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #Include /usr/local/etc/httpd/extra/httpd-info.conf\cf4 \cb1 \strokec4 \
\
\cf2 \cb3 \strokec2 # Virtual hosts\cf4 \cb1 \strokec4 \
\pard\pardeftab720\partightenfactor0
\cf4 \cb3 Include /usr/local/etc/httpd/extra/httpd-vhosts.conf\cb1 \
\
\pard\pardeftab720\partightenfactor0
\cf2 \cb3 \strokec2 # Local access to the Apache HTTP Server Manual\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #Include /usr/local/etc/httpd/extra/httpd-manual.conf\cf4 \cb1 \strokec4 \
\
\cf2 \cb3 \strokec2 # Distributed authoring and versioning (WebDAV)\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #Include /usr/local/etc/httpd/extra/httpd-dav.conf\cf4 \cb1 \strokec4 \
\
\cf2 \cb3 \strokec2 # Various default settings\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #Include /usr/local/etc/httpd/extra/httpd-default.conf\cf4 \cb1 \strokec4 \
\
\cf2 \cb3 \strokec2 # Configure mod_proxy_html to understand HTML4/XHTML1\cf4 \cb1 \strokec4 \
\pard\pardeftab720\partightenfactor0
\cf4 \cb3 <IfModule proxy_html_module>\cb1 \
\cb3 Include /usr/local/etc/httpd/extra/proxy-html.conf\cb1 \
\cb3 </IfModule>\cb1 \
\
\pard\pardeftab720\partightenfactor0
\cf2 \cb3 \strokec2 # Secure (SSL/TLS) connections\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #Include /usr/local/etc/httpd/extra/httpd-ssl.conf\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # Note: The following must must be present to support\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #       starting without SSL on platforms with no /dev/random equivalent\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #       but a statically compiled-in mod_ssl.\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\pard\pardeftab720\partightenfactor0
\cf4 \cb3 <IfModule ssl_module>\cb1 \
\cb3 SSLRandomSeed startup builtin\cb1 \
\cb3 SSLRandomSeed connect builtin\cb1 \
\cb3 </IfModule>\cb1 \
