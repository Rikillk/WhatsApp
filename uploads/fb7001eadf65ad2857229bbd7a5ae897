\outl0\strokewidth0 \strokec2 #\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # This is the main Apache HTTP server configuration file.  It contains the\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # configuration directives that give the server its instructions.\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # See <URL:http://httpd.apache.org/docs/2.4/> for detailed information.\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # In particular, see \cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # <URL:http://httpd.apache.org/docs/2.4/mod/directives.html>\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # for a discussion of each configuration directive.\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # Do NOT simply read the instructions in here without understanding\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # what they do.  They're here only as hints or reminders.  If you are unsure\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # consult the online docs. You have been warned.  \cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # Configuration and logfile names: If the filenames you specify for many\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # of the server's control files begin with "/" (or "drive:/" for Win32), the\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # server will use that explicit path.  If the filenames do *not* begin\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # with "/", the value of ServerRoot is prepended -- so "logs/access_log"\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # with ServerRoot set to "/usr/local/apache2" will be interpreted by the\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # server as "/usr/local/apache2/logs/access_log", whereas "/logs/access_log" \cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # will be interpreted as '/logs/access_log'.\cf4 \cb1 \strokec4 \
\
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # ServerRoot: The top of the directory tree under which the server's\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # configuration, error, and log files are kept.\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # Do not add a slash at the end of the directory path.  If you point\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # ServerRoot at a non-local disk, be sure to specify a local disk on the\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # Mutex directive, if file-based mutexes are used.  If you wish to share the\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # same ServerRoot for multiple httpd daemons, you will need to change at\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # least PidFile.\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\pard\pardeftab720\partightenfactor0
\cf4 \cb3 ServerRoot \cf5 \strokec5 "/usr/local/opt/httpd"\cf4 \cb1 \strokec4 \
\
\pard\pardeftab720\partightenfactor0
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # Mutex: Allows you to set the mutex mechanism and mutex file directory\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # for individual mutexes, or change the global defaults\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # Uncomment and change the directory if mutexes are file-based and the default\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # mutex file directory is not on a local disk or is not appropriate for some\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # other reason.\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # Mutex default:/usr/local/var/run/httpd\cf4 \cb1 \strokec4 \
\
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # Listen: Allows you to bind Apache to specific IP addresses and/or\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # ports, instead of the default. See also the <VirtualHost>\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # directive.\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # Change this to Listen on specific IP addresses as shown below to \cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # prevent Apache from glomming onto all bound IP addresses.\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #Listen 12.34.56.78:80\cf4 \cb1 \strokec4 \
\pard\pardeftab720\partightenfactor0
\cf4 \cb3 Listen 80\cb1 \
\
\pard\pardeftab720\partightenfactor0
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # Dynamic Shared Object (DSO) Support\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # To be able to use the functionality of a module which was built as a DSO you\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # have to place corresponding `LoadModule' lines at this location so the\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # directives contained in it are actually available _before_ they are used.\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # Statically compiled modules (those listed by `httpd -l') do not need\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # to be loaded here.\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # Example:\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 # LoadModule foo_module modules/mod_foo.so\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #\cf4 \cb1 \strokec4 \
